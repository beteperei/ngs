/*
  /* Crear una clase SingleChoice que herede de Question
  Numero maximo de answers 10
  */
class SingleChoice extends Question{
    constructor(titulo,answers){
        super(titulo);
        this.ansewrs=answers[9];
    }
    class SingleChoice extends Question{
    constructor(titulo,answers){
        super(titulo);
        this.ansewrs = [ ];
    }
    print() {
        console.log(`Titulo: ${this.titulo} `);
        console.log(`Respuestas: ${this.ansewrs}`)
    }
}
}
/**
   * Crear una clase MultipleChoice que herede de Questiom 
   * Numero maximo de answers 12
   */

class MultipleChoice extends Question{
    constructor(titulo, answers){
        super(titulo);
        this.answers = answers[11];
    }

}